    "ui-router": "./public/bower_components/angular-ui-router/release/angular-ui-router.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": "lodash"
    },
    "angularIcons": {
      "exports": "angular.module('ngMdIcons')",
      "depends": "angular"
    },
    "angularSortableView": {
      "exports": "angular.module('angular-sortable-view')",
      "depends": "angular"
    },
    "angularScroll": {
      "exports": "angular.module('luegg.directives')",
      "depends": "angular"
    },
    "angularAnimate": {
      "exports": "angular.module('ngAnimate')",
      "depends": "angular"
    },
    "angularAria": {
      "exports": "angular.module('ngAria')",
      "depends": "angular"
    },
    "angularMessages": {
      "exports": "angular.module('ngMessages')",
      "depends": "angular"
    },
    "angularMaterial": {
      "exports": "angular.module('ngMaterial')",
      "depends": [
        "angular",
        "angularAnimate",
        "angularAria"
      ]
    },
    "angularCookies": {
      "exports": "angular.module('ngCookies')",
      "depends": "angular"
    },
    "firebase": {
      "exports": "firebase",
      "depends": null
    },
    "chart": {
      "exports": "chart",
      "depends": null
    },
    "angularChart": {
      "exports": "angular.module('chart.js')",
      "depends": [
        "chart",
        "angular"
      ]
    },
    "angularFire": {
      "exports": "angular.module('firebase')",
      "depends": [
        "angular",
        "firebase"
      ]
    },
    "lodash": {
      "exports": "lodash",
      "depends": null
    },
    "holderjs": {
      "exports": "holderjs",
      "depends": null
    },
    "angularUploadcare": {
      "exports": "angular.module('ng-uploadcare')",
      "depends": "angular"
    },
    "angularHolderjs": {
      "exports": "angular.module('ngHolder')",
      "depends": [
        "angular",
        "holderjs"
      ]
    },
    "modernizr": {
      "exports": "modernizr",
      "depends": null
    },
    "angularBreadcrumb": {
      "exports": "angular.module('ncy-angular-breadcrumb')",
      "depends": "angular"
    },
    "ui-router": {
      "exports": "angular.module('ui.router')",
      "depends": "angular"
    }
  }
}
