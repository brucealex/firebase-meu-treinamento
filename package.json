  "repository": {
    "type": "git",
    "url": "https://github.com/brucealex/vittapro"
  },
  "main": "server.js",
  "engines": {
    "node": "0.12.x"
  },
  "scripts": {
    "start": "node server.js"
  },
  "dependencies": {
  },
  "devDependencies": {
    "bower": "~1.5.2",
    "brfs": "^1.4.0",
    "browser-sync": "^2.8.2",
